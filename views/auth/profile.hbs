<div class="container">
    <h1>Profile</h1>

    <div class="row">
        {{#with user}}
        <div class="col-md-3">
            <img src="{{this.path}}" alt="" style="width: 100%">
            <h4>{{this.username}} {{this.userLastname}}</h4>
            <h4>About me</h4>
            <p>{{this.aboutMe}}</p>
        </div>
        {{/with}}
    </div>
</div>